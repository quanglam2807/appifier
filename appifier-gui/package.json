{
  "name": "Appifier",
  "version": "11.0.0",
  "main": "public/electron.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps && node hacky.js",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./dist.js ./hacky.js ./public ./src --ext js",
    "start": "react-scripts start",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "dist-dev": "yarn build && node dist.js",
    "dist": "yarn build && cross-env NODE_ENV=production node dist.js",
    "test": "mocha ./tests"
  },
  "repository": "https://github.com/quanglam2807/appifier",
  "author": "Quang Lam <quang.lam2807@gmail.com>",
  "license": "MPL-2.0",
  "dependencies": {
    "appifier": "11.0.0",
    "electron-is-dev": "0.3.0",
    "electron-window-state": "4.1.1",
    "follow-redirects": "1.4.1",
    "fs-extra": "5.0.0",
    "is-url": "1.2.2",
    "tmp": "0.0.33",
    "yargs-parser": "9.0.2"
  },
  "devDependencies": {
    "algoliasearch": "3.24.12",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "classnames": "2.2.5",
    "concurrently": "3.5.1",
    "cross-env": "5.1.3",
    "electron": "1.8.2",
    "electron-builder": "20.2.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "material-ui": "1.0.0-beta.25",
    "material-ui-icons": "1.0.0-beta.35",
    "mocha": "5.0.1",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "react-scripts": "1.0.17",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "replace-in-file": "3.1.1",
    "request": "2.83.0",
    "semver": "5.5.0",
    "spectron": "3.8.0",
    "typeface-roboto": "0.0.54",
    "wait-on": "2.1.0"
  },
  "homepage": "./",
  "private": true
}
